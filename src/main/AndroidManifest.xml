<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" package="com.demo.wakemeup" android:versionCode="1" android:versionName="0.0.0.1" android:installLocation="auto">
    <uses-feature android:required="true" android:glEsVersion="0x00020000"/>
